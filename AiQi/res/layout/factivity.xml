<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
<ImageView
        android:id="@+id/imageView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/dddg" />

    
      <ListView
    android:id="@+id/list"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
      android:layout_above="@+id/dilan"
    android:layout_below="@+id/gallery1" 
     android:divider="#00000000"
            android:cacheColorHint="#00000000" 
            android:listSelector="@drawable/listitem_background"
  >
    </ListView>
      

       
    <Gallery
        android:id="@+id/gallery1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/aiqi"
        android:spacing="5px"
        android:unselectedAlpha="0.6" />
    

    <TextView
        android:id="@+id/aiqi"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#ffeeff00"
        android:gravity="center"
        android:text="爱骑"
        android:textSize="28sp" />

    <ImageButton
        android:id="@+id/paishe"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/aiqi"
        android:layout_alignParentLeft="true"
        android:background="@android:drawable/ic_menu_camera" />

    <ImageButton
        android:id="@+id/sousuo"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/aiqi"
        android:layout_alignParentRight="true"
        android:background="@android:drawable/ic_search_category_default" />

    <TextView
        android:id="@+id/dilan"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="#ffeeff00" />


    <ImageButton
        android:id="@+id/shouye"
        android:layout_width="30dp"
        android:layout_height="30dp"
  
        android:layout_alignTop="@+id/dilan"
        android:layout_marginLeft="10dp"

        android:src="@android:drawable/ic_menu_save"
        android:background="#00feee" 
          />

    <ImageButton
        android:id="@+id/add"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignBottom="@+id/shouye"
        android:layout_centerHorizontal="true"
        android:background="@android:drawable/ic_menu_add" />

    <ImageButton
        android:id="@+id/xiaoxi"
        android:layout_width="30dp"
        android:layout_height="30dp"
   android:layout_above="@+id/textxx"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/add"
        android:background="@android:drawable/stat_notify_chat" />

    <ImageButton
        android:id="@+id/tongcheng"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textsy"
        android:layout_marginRight="30dp"
        android:layout_toLeftOf="@+id/add"
        android:background="@android:drawable/ic_menu_myplaces" />

    <TextView
        android:id="@+id/texttc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tongcheng"
        android:layout_below="@+id/tongcheng"
        android:text="同城"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/textsy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/shouye"
        android:layout_below="@+id/shouye"
        android:text="首页"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/textfb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/texttc"
        android:layout_centerHorizontal="true"
        android:text="发布"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textxx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textfb"
        android:layout_alignRight="@+id/xiaoxi"
        android:text="消息"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/textwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textfb"
        android:layout_alignBottom="@+id/textfb"
        android:layout_alignLeft="@+id/geren"
        android:text="我的"
        android:textSize="13sp" />


    <ImageButton
        android:id="@+id/geren"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textxx"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:background="@android:drawable/stat_notify_voicemail" />

    

</RelativeLayout>